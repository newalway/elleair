{% extends "ProjectBundle:"~ view_layout ~":layout.html.twig" %}

{% block title %}{% trans %}acseine.breadcrumb.history{% endtrans %} | {% trans %}project.title_bar_name{% endtrans %} {{ parent() }}{% endblock %}
{% block home_active %}active{% endblock %}
{% block m_home_active %}active{% endblock %}

{% block main_class %}{% endblock %}

{% block breadcrumb %}
<div class="breadcrumb">
	<div class="container breadcrumb_in">
		<div class="breadcrumb_items">
			<a href="{{path('homepage')}}" class="active">{% trans %}acseine.breadcrumb.top{% endtrans %}</a>
		</div>
		<div class="breadcrumb_items">
			<a href="{{path('about')}}" class="active">{% trans %}acseine.breadcrumb.about{% endtrans %}</a>
		</div>
		<div class="breadcrumb_items">
			{% trans %}acseine.breadcrumb.history{% endtrans %}
		</div>
	</div>
</div>
<!-- /breadcrumb -->
<div class="container title_category">
	<h1>{% trans %}acseine.breadcrumb.history{% endtrans %}</h1>
</div>
{% endblock %}

{% block content %}
<section id="history" class="history">
<!-- /title_table -->
<div class="main-content container">
	<div class="history_list">
		{% if histories|length %}
			{% for data in histories %}
			    <div class="history_items">
					<div class="number">{{data.getTitle}}&nbsp;&#x2015;</div>
					<div class="description">
						{{data.getDescription|raw}}
					</div>
				</div>
			{% endfor %}
		{% endif %}
	</div>
</div>
</section>
{% endblock %}
