<div class="main">
	{% block breadcrumb %}{% endblock %}
	{% block content %}{% endblock %}
</div>{#end main#}


	<footer class="footer">
	  <div class="footer-nav-sp is-sp">
		<ul class="nav01">
			<li><a href="./series">Product series</a></li>
