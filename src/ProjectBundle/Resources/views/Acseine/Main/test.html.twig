{% block html_keywords %}{% endblock %}

{% block html_meta %}

{% endblock %}

{% block stylesheets %}
	{{ parent() }}
{############################# original #############################################}
<link href="https://fonts.googleapis.com/css?family=Noto+Sans" rel="stylesheet">
<link rel="stylesheet" href="{{asset('acseine/font-awesome-4.7.0/css/font-awesome.min.css')}}">
<link rel="stylesheet" href="{{asset('acseine/original/common/css/reset.css')}}">
<link rel="stylesheet" href="{{asset('acseine/original/common/css/global.css')}}">
<link rel="stylesheet" href="{{asset('acseine/original/common/css/module.css')}}">


<link rel="stylesheet" type="text/css" href="{{asset('acseine/original/css/slick.css')}}">
<link rel="stylesheet" type="text/css" href="{{asset('acseine/original/css/styles.min.css')}}">
