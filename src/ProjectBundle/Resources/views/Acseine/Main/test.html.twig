{% endblock %}

{% block body %}
<div id="wrapper" class="wrap">
<header class="header">
	<div class="container">
		<div class="container header-top">
			<h1><a href="./"><img src="{{asset('acseine/original/common/images/logo.png')}}" alt="logo"></a></h1>
			<div class="header-right no-sp">
				<div class="header-lang">
					{% if app.request.getLocale() == 'en' %}
						<span><img class="flag-lang2" src="{{ asset('images/marathon/flat-united-states-of-america.png')}}" alt="English" />English</a></span>
					{% elseif app.request.getLocale() == 'th' %}
						<span><img class="flag-lang2" src="{{ asset('images/marathon/flat-thailand.png')}}" alt="ภาษาไทย" />ภาษาไทย</a></span>
					{% else %}
						<span>Language</span>
					{% endif %}
					<ul class="ul-lang">
						<li><a href="http://www.acseine.co.jp/" target="_blank"><img src="{{asset('acseine/original/common/images/japan.jpg')}}" alt="japan"> Japan</a></li>
						<li><a href="{{path(app.request.attributes.get('_route'), (app.request.attributes.get('_route_params')|merge({'_locale': 'en'}))|merge(app.request.query.all))}}" title="English"><img class="flag-lang" src="{{ asset('images/marathon/flat-united-states-of-america.png')}}" alt="English" />English</a></li>
						<li><a href="{{path(app.request.attributes.get('_route'), (app.request.attributes.get('_route_params')|merge({'_locale': 'th'}))|merge(app.request.query.all))}}" title="ภาษาไทย"><img class="flag-lang" src="{{ asset('images/marathon/flat-thailand.png')}}" alt="ภาษาไทย" />ภาษาไทย</a></li>
					</ul>
				</div>
				<div class="header-social">
					<a href="https://www.facebook.com/acseine.co.jp/"><img src="{{asset('acseine/original/common/images/facebook_icon.png')}}" alt=""></a>
				</div>
			</div>
			<div class="header-btn-nav is-sp">
				<div class="btn-icon">
				  <span></span>
				  <span></span>
				  <span></span>
				</div>
			</div>
		</div>
		<div class="header-nav">
		  <div class="header-nav-content">
			<div class="header-nav-top">
				<ul>
						<li>
							<a href="{{path('homepage')}}">{% trans %}acseine.menu.top{% endtrans %}</a>
						</li>
						<li>
							<a href="{{path('product')}}">{% trans %}acseine.menu.product{% endtrans %}</a>
						</li>
						<li>
							<a href="{{path('about')}}">{% trans %}acseine.menu.about{% endtrans %}</a>
						</li>
						<li>
							<a href="{{path('counseling')}}">{% trans %}acseine.menu.counseling{% endtrans %}</a>
						</li>
						<li>
							<a href="{{path('skin_problems_and_cosmetics')}}">{% trans %}acseine.menu.skincosmetics{% endtrans %}</a>
						</li>
						<li>
							<a href="{{path('shop')}}">{% trans %}acseine.menu.shoplist{% endtrans %}</a>
						</li>
					</ul>
			</div>
				<div class="header-nav-bottom is-sp">
			<div class="header-lang">
						{% if app.request.getLocale() == 'en' %}
							<span><img class="flag-lang2" src="{{ asset('images/marathon/flat-united-states-of-america.png')}}" alt="English" />English</a></span>
						{% elseif app.request.getLocale() == 'th' %}
							<span><img class="flag-lang2" src="{{ asset('images/marathon/flat-thailand.png')}}" alt="ภาษาไทย" />ภาษาไทย</a></span>
						{% else %}
							<span>Language</span>
						{% endif %}
						<ul class="ul-lang">
							<li><a href="http://www.acseine.co.jp/" target="_blank"><img src="{{asset('acseine/original/common/images/japan.jpg')}}" alt="japan"> Japan</a></li>
							<li><a href="{{path(app.request.attributes.get('_route'), (app.request.attributes.get('_route_params')|merge({'_locale': 'en'}))|merge(app.request.query.all))}}" title="English"><img class="flag-lang" src="{{ asset('images/marathon/flat-united-states-of-america.png')}}" alt="English" />English</a></li>
							<li><a href="{{path(app.request.attributes.get('_route'), (app.request.attributes.get('_route_params')|merge({'_locale': 'th'}))|merge(app.request.query.all))}}" title="ภาษาไทย"><img class="flag-lang" src="{{ asset('images/marathon/flat-thailand.png')}}" alt="ภาษาไทย" />ภาษาไทย</a></li>
						</ul>
					</div>
					<div class="header-social">
						<a href="https://www.facebook.com/acseine.co.jp/"><img src="{{asset('acseine/original/common/images/facebook_icon.png')}}" alt=""></a>
					</div>
				</div>
		  </div>
		</div>
	</div>
