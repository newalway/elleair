{% extends "ProjectBundle:"~ view_layout ~":layout.html.twig" %}

{% block title %} {{data.getTitle}} | {% trans %}project.title_bar_name{% endtrans %} | {% trans %}project.title_bar_name{% endtrans %} {{ parent() }}{% endblock %}
{% block how_to_buy_active %}active{% endblock %}
{% block m_how_to_buy_active %}active{% endblock %}

{% block html_description %}{% trans %}acseine.how_to_buy.sub_head1{% endtrans %}{% endblock %}
