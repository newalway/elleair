{% extends "ProjectBundle:"~ view_layout ~":layout.html.twig" %}

{% block title %}{% trans %}acseine.company_profile.title{% endtrans %} | {% trans %}project.title_bar_name{% endtrans %} {{ parent() }}{% endblock %}
{% block home_active %}active{% endblock %}
{% block m_home_active %}active{% endblock %}

{% block html_description %}{% trans %}acseine.company_profile.sub_head1{% endtrans %}{% endblock %}
{% block html_keywords %}{% trans %}acseine.company_profile.html_keywords{% endtrans %}{% endblock %}

{% block main_class %}{% endblock %}

{% block breadcrumb %}
<div class="breadcrumb">
	<div class="container breadcrumb_in">
		<div class="breadcrumb_items">
			<a href="{{path('homepage')}}" class="active">{% trans %}acseine.breadcrumb.top{% endtrans %}</a>
		</div>
		<div class="breadcrumb_items">
			{% trans %}acseine.breadcrumb.company_profile{% endtrans %}
		</div>
	</div>
</div>
<!-- /breadcrumb -->
<div class="container title_category">
	<h1>{% trans %}acseine.company_profile.title{% endtrans %}</h1>
</div>
{% endblock %}

{% block content %}
<section id="company-profile" class="company-profile">
			<!-- /title_table -->
<div class="main-content container">
	<div class="table_about">
		<div class="table_about_group_table">
			<div class="table_about_content">
				<div class="table_about_table">
				    <table>
				        <tbody>
				            <tr>
				                <td>{% trans %}acseine.company_profile.company_name_title{% endtrans %}</td>
				                <td>{% trans %}acseine.company_profile.company_name_data{% endtrans %}</td>
				            </tr>
				            {#<tr>
				                <td>{% trans %}acseine.company_profile.representative_title{% endtrans %}</td>
				                <td>{% trans %}acseine.company_profile.representative_data{% endtrans %}</td>
				            </tr>#}
				            <tr>
				                <td>{% trans %}acseine.company_profile.business_description_title{% endtrans %}</td>
				                <td>{% trans %}acseine.company_profile.business_description_data{% endtrans %}</td>
				            </tr>
				            {#<tr>
				                <td>{% trans %}acseine.company_profile.established_title{% endtrans %}</td>
				                <td>{% trans %}acseine.company_profile.established_data{% endtrans %}</td>
				            </tr>#}
				            <tr>
				                <td>{% trans %}acseine.company_profile.head_office_title{% endtrans %}</td>
				                <td>{% trans %}acseine.company_profile.head_office_data{% endtrans %}</td>
				            </tr>
				        </tbody>
				    </table>
				</div>
			</div>
		</div>
				    <!-- /table_about_group_table -->
	</div>
</div>

</section>

{% endblock %}
