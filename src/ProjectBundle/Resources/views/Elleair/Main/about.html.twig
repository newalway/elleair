{% extends "ProjectBundle:"~ view_layout ~":layout.html.twig" %}

{% block title %}{% trans %}menu.about.us{% endtrans %} | {{ parent() }}{% endblock %}
{% block about_active %}active{% endblock %}
{% block introduction_active %}active{% endblock %}

{% block breadcrumb_bg %}{{asset('elleair/images/bg/bg03.jpg')}}{% endblock %}
{% block breadcrumb_title %}{% trans %}menu.about.us{% endtrans %}{% endblock %}
{% block breadcrumb %}
    <li class="active">{% trans %}menu.about.us{% endtrans %}</li>
{% endblock %}

{% block breadcrumb_data_opacity %}4{% endblock %}
{% block breadcrumb_data_overlay %}blue{% endblock %}

{% block javascripts %}
	{{ parent() }}
{% endblock %}

{% block content %}
<div class="candidate-section pt-100 pb-65">
    <div class="container">
        <div class="row align-items-center">

            <div class="candidate-content col-lg-6 col-12 mb-30">
                <h1 class="text-center">{% trans %}elleair.introduction.our_mission{% endtrans %}</h1>
                <p style="color:#0B64A2">
                    {% trans %}elleair.introduction.our_mission_description{% endtrans %}
                </p>
            </div>

            <div class="col-lg-6 col-12 mb-30">
                <img src="{{asset('elleair/images/about/p02-2.png')}}" alt="" class="img-fluid">
            </div>

        </div>
    </div>
</div>

