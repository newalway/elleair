<!-- Testimonial Section Start -->
<div class="testimonial-section world-section">
    <div class="container">
        <div class="row">
            <div class="col-lg-10 col-12 ml-auto mr-auto">

                <!-- Testimonial Slider Start -->
                <div class="testimonial-slider" style="background-image: url({{asset('elleair/images/bg/testimonial-bg.png')}})">

                    <!-- Testimonial Item -->
                    <div class="testimonial">
                        <h2>{% trans %}elleair.introduction.company_name{% endtrans %}</h2>
                        <p>{% trans %}elleair.introduction.company_description{% endtrans %}</p>
                        <img src="{{asset('elleair/images/history/p01.png')}}"  />
                    </div>

                </div>
				<!-- Testimonial Slider End -->

            </div>
        </div>
    </div>
</div><!-- Testimonial Section End -->

<div class="mission-section bg-gray pt-100 pb-95">
    <div class="container">
        <div class="row align-items-center">
            <!-- Mission Content Start -->
            <div class="col-lg-6 col-12 order-lg-2">
